{
  "extends": "@tsconfig/node14/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "isolatedModules": true,
    "lib": ["ES2020"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": false,
    "target": "ES5",
    "types": ["node"],
    "useUnknownInCatchVariables": false
  }
}
